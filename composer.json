{
    "name": "anatoliynyatin/far-symfony2-jquery-upload",
    "description": "Integrates JqueryUpload into Symfony2 with OneUpFlySystem",
    "type": "symfony-bundle",
    "keywords" : [
      "Bundle JqueryUpload FlySystem Symfony2"
    ],
    "license": "MIT",
    "authors": [
      {
        "name": "Fernando Aparicio",
        "email": "faparicior@gmail.com",
        "homepage": "http://www.fernandoaparicio.net",
        "role": "Developer"
      }
    ],
    "autoload": {
        "psr-4": {
            "faparicior\\FARSymfony2UploadBundle\\": ""
        }
    },
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "3.3.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "^3.0.2",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/rest-bundle": "^2.2",
        "imagine/imagine": "^0.6.3",
        "jms/serializer-bundle": "^2.0",
        "oneup/flysystem-bundle": "^1.2",
        "symfony/finder": "^3.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}
